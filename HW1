function findMaxNumber(arr) {
  var maxNumber = arr[0]; // Предполагаем, что первый элемент является максимальным

  for (var i = 1; i < arr.length; i++) {
    if (arr[i] > maxNumber) {
      maxNumber = arr[i]; // Если текущий элемент больше максимального, обновляем максимальное число
    }
  }

  return maxNumber;
}

var numbers = [5, 8, 2, 17, 1, 4];
console.log(findMaxNumber(numbers));
